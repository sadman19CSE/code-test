#include <stdio.h>
int main()
{
    int n, digit, reverse = 0;
    
        printf("Enter a number: ");

        scanf("%d", &n);

        int original = n;

       while(n != 0){

        digit = n % 10;

        reverse = reverse*10 + digit;

        n = n/10;

        }

        if(reverse == original){

       printf("The number is Palindrome\n");

        }else{

            printf("The number is not Palindrome\n");

        }


       return 0;
}
