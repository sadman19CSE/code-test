#include <stdio.h>
#include<math.h>

int main()
{
    int n, digit;
    double sum;
    
        printf("Enter a number: ");

        scanf("%d", &n);

        int original = n;      

       while(n != 0){

        digit = n % 10;

        sum += pow(digit, 3);    

        n = n/10;

        }

        if(sum == original){

       printf("The number is Armstrong\n");

        }else{

            printf("The number is not Armstrong\n");

        }
       return 0;
}
