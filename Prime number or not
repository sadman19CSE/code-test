#include <stdio.h>

int main()
{

    int n,count = 0;

    printf("Enter a number: ");

    scanf("%d", &n);

    for (int i = 2; i < n; i++)
    
    {

        if (n % i == 0)
        {
         count++;
         break;
        }      
    }
if(count == 0)

printf("Prime number\n");

else
printf("Not a prime number\n");

    return 0;
}
