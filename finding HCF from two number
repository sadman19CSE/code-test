#include <stdio.h>

    int gcd(int a, int b) {

int gcd = 1;

for(int i = 1; i <= a && i <=b; i++) {

if(a % i == 0 && b % i == 0)

gcd = i;

}
    return gcd;

}
int main () {

int num1,num2,result;

printf("Enter two number = ");
scanf("%d %d", &num1, &num2);

result = gcd(num1,num1);

printf("GCD = %d",result);


return 0;
}
